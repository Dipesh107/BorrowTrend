<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>The Borrow Trend | Clothes</title>
  <link rel="stylesheet" href="css/clothes.css">
</head>

<body>
  <div class="head-section">
    <div>
      <img class="head-section-image" src="images/logo-black-300.gif" alt="The Borrow Trend">
    </div>
    <div>
      <!-- Navigation menu -->
      <%- include('includes/nav') %>
    </div>
  </div>

  <div class="hero-section">
    <div class="clothes-image">
      <img class="image-1" src="images/clothes.png" alt="demo-1">
    </div>
    <div class="clothes-image">
      <img class="image-2" src="images/clothes2.png" alt="demo-2">
    </div>
  </div>

  <div class="companies">
    <div>
      <img src="images/logos-comp/logo-1.png" alt="">
      <img src="images/logos-comp/logo-2.png" alt="">
      <img src="images/logos-comp/logo-3.png" alt="">
      <img src="images/logos-comp/logo-4.png" alt="">
      <img src="images/logos-comp/logo-5.png" alt="">
      <img src="images/logos-comp/logo-6.png" alt="">
    </div>
  </div>

  <div class="discounts">
    <!-- <img class="discount-images" name="slide" width="100%" height="100%"> -->
    <img src="images/kotak.png" style="width: 100%;" alt="discount-image">
  </div>

  <!-- Features Section -->
  <%- include('includes/features') %>

    <div class="product-cards">
      <%- include('includes/cards') %>
    </div>



    <!-- testimonial section -->
    <%- include('includes/testimonial') %>

      <!-- footer -->
      <%- include('includes/footer') %>

        <script>
          var i = 0;
          var images = [];
          var time = 3000;
          images[0] = "images/icici.png"
          images[1] = "images/kotak.png"
          function changeImage() {
            document.slide.src = images[i];
            if (i < images.length == 1) {
              i++;
            } else {
              i = 0;
            }
            setTimeout("changeImage()", time);
          }
          window.onload = changeImage;
        </script>
</body>

</html>